    :hierarchyLevel "4" .

:a3b3ff82-cdf2-df26-83e0-3b546480ae08 a :InspectionPoint ;
    rdfs:label "Einstreu"@de,
        "FR: Einstreu"@fr,
        "Lettiera"@it ;
    rdfs:comment "Einstreumenge ermöglicht den Tieren zu scharren"@de,
