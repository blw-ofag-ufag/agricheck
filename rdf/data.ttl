    :hierarchyLevel "4" .

:a3b3ff82-d157-1576-83e0-3b546480ae08 a :InspectionPoint ;
    rdfs:label "Biobetrieb anerkannt"@de,
        "FR: Biobetrieb anerkannt"@fr,
