:a3b3ff82-cfcc-fcc6-83e0-3b546480ae08 a :InspectionPoint ;
    rdfs:label "max. Anzahl Labelplätze Masttruten BTS"@de,
        "FR: max. Anzahl Labelplätze Masttruten BTS"@fr,
